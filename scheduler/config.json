{
  "name": "Scheduler",
  "version": "0.0.13",
  "slug": "scheduler",
  "description": "Manage your Docker environment with ease",
  "url": "https://github.com/hassio-addons/addon-scheduler/",
  "advanced": true,
  "ingress": true,
  "ingress_port": 8099,
  "startup": "application",
  "panel_icon": "mdi:calendar-clock",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "init": false,
  "map": [
    "ssl",
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "8099/tcp": null
  },
  "ports_description": {
    "8099/tcp": "Edge Agent Api (Enable when managing remote edge agents)"
  },
  "boot": "auto",
  "stdin": true,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "docker_api": true,
  "options": {
    "log_level": "info",
	  "max_retries": 2,
	  "max_retry_interval": 5,
	  "bk_color": "transparent"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "agent_secret": "str?",
    "max_retries": "int(1,10)",
	  "max_retry_interval": "int(1,120)",
	  "bk_color": "str"
  },
  "webui": "http://[HOST]:[PORT:8099]"
}